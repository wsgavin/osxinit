export PATH="$HOME/bin:$PATH"
export PATH="/usr/local/bin:$PATH"
export PATH="/usr/local/sbin:$PATH"

# The following path additions are to support coreutils.
export PATH="/usr/local/opt/coreutils/libexec/gnubin:$PATH"
export MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"

# The following path additions are to support gnu-sed.
export PATH="/usr/local/opt/gnu-sed/libexec/gnubin:$PATH"
export MANPATH="/usr/local/opt/gnu-sed/libexec/gnuman:$MANPATH"

# Adds additional php binaries.
export PATH="/usr/local/opt/php56/bin:$PATH"

# Adds MacDown commandline utility
export PATH="/Applications/MacDown.app/Contents/SharedSupport/bin:$PATH"

